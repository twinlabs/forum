{
  "name": "f.orum",
  "version": "2.0.0-0",
  "repository": {
    "type": "git",
    "url": "https://github.com/twinlabs/forum/"
  },
  "main": "app.js",
  "engines": {
    "node": "6.x.x"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "scripts": {
    "bundle:lib": "./node_modules/.bin/browserify --require react --require react-dom --require redux --require \"lodash/core\" --require superagent --require socket.io-client > ./build/lib.js",
    "bundle:app:watch": "node_modules/.bin/watchify app/index.js --external react-dom --external react --external redux --external \"lodash/core\" --external superagent --external socketio-client -t babelify -p livereactload -o build/bundle.js",
    "check": "./node_modules/.bin/eslint --ext .js,.jsx app/",
    "db:seed": "node db/seed.js",
    "deploy": "git push heroku deploy:master",
    "test": "NODE_ENV=test mocha --require test/test_helper.js --compilers jsx:babel-register --recursive ./test && npm run check",
    "test:watch": "NODE_ENV=test mocha --require test/test_helper.js --compilers jsx:babel-register --recursive --watch --reporter min ./test",
    "test:debug": "NODE_ENV=test node-debug _mocha --require test/test_helper.js --compilers jsx:babel-register --recursive --watch --reporter min ./test",
    "start": "concurrently \"npm run bundle:lib\" \"node app.js\" \"npm run bundle:app:watch\"",
    "start:debug": "node-debug app.js"
  },
  "dependencies": {
    "autoprefixer": "^6.0.0",
    "babel": "^6.0.0",
    "babel-core": "^6.9.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.9.0",
    "babelify": "^7.3.0",
    "bcrypt": "~0.8.0",
    "bluebird": "^3.4.6",
    "body-parser": "^1.12.2",
    "browserify": "^13.0.0",
    "classnames": "^2.2.5",
    "compression": "^1.4.3",
    "concurrently": "^2.1.0",
    "connect-pg-simple": "^2.1.1",
    "content.css": "^3.1.2",
    "cookie-parser": "^1.3.4",
    "ejs": "^2.3.4",
    "epilogue": "0.3.2",
    "express": "^4.12.3",
    "express-session": "1.7.6",
    "favico.js": "^0.3.10",
    "history": "^2.0.0",
    "horsey": "^2.6.1",
    "less-middleware": "1.0.4",
    "livereactload": "^2.2.3",
    "lodash": "^4.13.1",
    "marked": "git://github.com/twinlabs/marked.git",
    "moment": "~2.5.0",
    "multer": "^0.1.8",
    "normalize.css": "^4.2.0",
    "passport": "~0.2.0",
    "passport-local": "~1.0.0",
    "pg": "^4.5.5",
    "pg-hstore": "^2.3.1",
    "postcss": "^5.2.6",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-proxy": "^1.1.8",
    "react-router": "^2.8.0",
    "redux": "^3.5.2",
    "sequelize": "~2.0.3",
    "socket.io": "~1.3.5",
    "socket.io-client": "~1.3.5",
    "superagent": "^2.0.0",
    "watchify": "^3.7.0"
  },
  "devDependencies": {
    "dedent": "^0.6.0",
    "eslint": "^3.7.0",
    "eslint-plugin-react": "^6.3.0",
    "jsdom": "^9.6.0",
    "mocha": "^3.0.0",
    "node-inspector": "^0.12.8",
    "react-addons-test-utils": "^15.3.2",
    "request": "^2.49.0",
    "sinon": "^1.17.0"
  }
}
